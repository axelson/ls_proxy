<div class="row">
  <button phx-click="reset">Reset</button>
</div>

<div class="row">
  <div class="column">
    <h1>Messages</h1>
    <%= for message_record <- @message_records do %>
      <div class="message">
        <%= render_message(message_record.message) %>
      </div>
    <% end %>
  </div>
</div>
